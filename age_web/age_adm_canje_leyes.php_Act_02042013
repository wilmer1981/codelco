				case "3":
					$Seguimiento='TOMA LEY DEL PQTE 3<br>';
					$ArrayValorLeyes=array($ValorLey1,$ValorLey2,$ValorLey3);
					sort($ArrayValorLeyes);
					if(($ValorLey3>$ValorLey2)||($ValorLey3<$ValorLey1))
