	}
	if($Calcular=='S')//CALCULA EL CANJE
	{
		$Datos=explode('//',$Valores);
		while(list($c,$v)=each($Datos))
		{
			$Datos2=explode('~~',$v);
			$CodLey=$Datos2[0];
			$NomLey=$Datos2[1];
			$NomUnidad=$Datos2[2];
			$ValorLey1=$Datos2[3]*1;
			$ValorLey2=$Datos2[4]*1;
			$ValorLey3=$Datos2[5]*1;
			$ValorLey4=$Datos2[6]*1;
			$ValorLeyR=$Datos2[7]*1;
			$ValorIncR=$Datos2[8]*1;
