function AsignarEnvio()
{
	var Frm=document.FrmProgLoteo;

	Valores=RecuperarValores();	
	if (Valores!="")
	{
		if (confirm('�Esta Seguro de Asignar el Envio Y Seleccionar El Transportista?'))
		{
			//Frm.action="sec_distribuir_lote01.php?Proceso=E&Tipo=V&Valores="+Valores;
			//Frm.submit();
			window.open("sec_transportista2.php?&Valores="+Valores,""," top=5,left=5,fullscreen=no,width=750,height=560,scrollbars=yes,resizable = yes");
		}	
	}
} 

function RecuperarValores()
{
	var Frm=document.FrmProgLoteo;
	var Valores=new String();
	
	for (i=1;i<Frm.OptSeleccionar.length;i++)
	{
		if (Frm.OptSeleccionar[i].checked==true)
		{
			Valores=Valores + Frm.OptSeleccionar[i].value +"//";
		//	alert (Frm.OptSeleccionar[2].value);
		}
	}
	if (Valores!='')
	{
		Valores=Valores.substr(0,Valores.length-2);
		return(Valores);	
	}
	else
	{
		Valores="";
		return(Valores);	
	}	
} 

function Salir()
{
	var Frm=document.FrmProgLoteo;
	Frm.action="../principal/sistemas_usuario.php?CodSistema=3";
	Frm.submit();
	
}
function Imprimir(opt)
{
	var f=document.FrmProgLoteo;
	switch (opt)
	{
		case "W":
			window.open("sec_programa_adm_loteo_imp_web.php", "","menubar=no resizable=yes Top=30 Left=50 width=670 height=500 scrollbars=yes");
			 break;
	}
}
function EliminaVirtual(Opc,CorrVirtual,TipoIE)
{
	var f=document.FrmProgLoteo;
	var mensaje='';
	mensaje=confirm('�Est� seguro de Eliminar Intrucci�n Virtual?')
	if(mensaje==true)
	{
		f.action="sec_programa_adm_loteo01.php?ElimVirtual=S&CorrEnmVirElim="+CorrVirtual+"&TipoIE="+TipoIE;
		f.submit();
	}
}
