			switch ($TipoIE)
			{
			
				case "Normal":
					$CrearTmp ="create temporary table if not exists sec_web.tmpprograma "; 
					$CrearTmp =$CrearTmp."(corr_ie bigint(8),cliente_nave varchar(30),fecha date,fecha_programacion date,";
